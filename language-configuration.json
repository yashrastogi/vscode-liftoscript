{
    "comments": {
        "lineComment": "//"
    },
    "brackets": [
        [
            "{",
            "}"
        ],
        [
            "[",
            "]"
        ],
        [
            "(",
            ")"
        ]
    ],
    "wordPattern": "(-?\\d*\\.\\d\\w*)|([^\\`\\~\\!\\@\\#\\%\\^\\&\\*\\(\\)\\-\\=\\+\\[\\{\\]\\}\\\\\\|\\;\\:\\'\\\"\\,\\.\\<\\>\\/\\?\\s]+)",
    "indentationRules": {
        "increaseIndentPattern": "^((?!\\#).)*(\\{[^}\\\"'`]*|\\([^)\\\"'`]*|\\[[^]\\\"'`]*)$",
        "decreaseIndentPattern": "^((?!.*[}\\]\\)]).*)$"
    },
    "folding": {
        "markers": {
            "start": "^#+\\s.*$",
            "end": "^$"
        }
    }
}